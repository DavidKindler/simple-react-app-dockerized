{
  "name": "simple-react-app-dockerized",
  "version": "1.0.0",
  "description": "Simple React app that is dockerized",
  "keywords": ["react", "docker", "compose", "edit", "webpack", "babel"],
  "main": "index.js",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "start": "webpack-dev-server -w",
