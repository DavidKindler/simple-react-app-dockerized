    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "react": "^15.5.0",
    "react-dom": "^15.5.0"
  }
